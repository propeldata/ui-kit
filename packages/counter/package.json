{
  "name": "@propeldata/wc-counter",
  "version": "0.1.0",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "dev": "tsc -p tsconfig.cjs.json --watch",
    "build": "yarn build:cjs && yarn build:esm",
    "build:esm": "rimraf dist/esm tsc -p tsconfig.esm.json",
    "build:cjs": "rimraf dist/cjs tsc -p tsconfig.cjs.json",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'lib/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "lit-analyzer": "^1.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@propeldata/wc-plugins": "workspace:^",
    "chart.js": "^4.0.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "lit": "^2.4.1"
  },
  "peerDependencies": {
    "@propeldata/wc-plugins": "workspace:^",
    "chart.js": "^4.0.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0"
  },
  "customElements": "custom-elements.json"
}
